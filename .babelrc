{
  "presets": [
    "@babel/env"
  ],

  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-export-default-from",
    "@babel/proposal-export-namespace-from",
    ["@babel/transform-runtime", {
      "corejs": 3,
      "helpers": false,
      "regenerator": true,
      "useESModules": true
    }],
  ]
}